[
    {
        "name": "mongodb",
        "image": "mongo:latest",
        "cpu": 10,
        "memory": 256,
        "portMappings": [
            {
                "containerPort": 27017,
                "hostPort": 0,
                "protocol": "tcp"
            }
        ],
        "command": [
            "mongod",
            "--auth"
        ],
        "environment": [
            {
                "name": "MONGO_INITDB_ROOT_USERNAME",
                "value": "root"
            },
            {
                "name": "MONGO_INITDB_ROOT_PASSWORD",
                "value": "root"
            }
        ]
    }
]