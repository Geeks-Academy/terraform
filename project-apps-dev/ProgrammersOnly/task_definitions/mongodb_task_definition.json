[
    {
        "name": "mongo",
        "image": "mongo:latest",
        "cpu": 10,
        "memory": 256,
        "portMappings": [
            {
                "containerPort": 27017,
                "hostPort": 27017,
                "protocol": "tcp"
            }
        ],
        "command": ["mongod", "--auth"],
        "environmentFiles": [
            {
                "value": "arn:aws:s3:::programmers-only-artifacts/env-files/mongo.env",
                "type": "s3"
            }
        ]
    }
]